<launch>
    <arg name="veh"/>
    <arg name="param_file_name" default="default" doc="Specify a param file. ex:megaman"/>
    <arg name="param_file_name_wheels_driver_node" default="default"
         doc="Specify a param file. ex:megaman"/>
    <arg name="pkg_name" default="pure_pursuit_carrot" doc="name of the package"/>
    <arg name="node_name" default="pure_pursuit_carrot_node" doc="name of the node"/>

    <group ns="$(arg veh)">
        <node pkg="$(arg pkg_name)" type="$(arg node_name).py" name="$(arg node_name)"
              output="screen">
            <param name="veh" value="$(arg veh)" />
            <rosparam command="load"
                      file="$(find pure_pursuit_carrot)/config/$(arg node_name)/$(arg param_file_name).yaml"/>
        </node>
    </group>

</launch>
